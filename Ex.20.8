Public Pair:
package com.company;
import java.util.Scanner;
import java.lang.String;
import java.util.*;
import java.lang.Exception;

public class Pair <F, S> {
    private F First;
    private S Second;

    public Pair(F First, S Second)
    {
        this.First = First;
        this.Second = Second;
    }
    public void setFirst(F First){
        this.First = First;
    }
    public F getFirst(){
        return First;
    }
    public void setSecond(S Second){
        this.Second = Second;
    }
    public S getSecond(){
        return Second;
    }
}

